<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TextArea?>
<GridPane fx:controller="sample.Controller"
          xmlns:fx="http://javafx.com/fxml" alignment="center" hgap="10" vgap="10">
    <Button GridPane.columnIndex="1"
            GridPane.rowIndex="1"
            text="TestButton" onAction="#testFunctie" />
    <Button GridPane.columnIndex="2"
            GridPane.rowIndex="1"
            text="InactiefVanaf query" onAction="#InactiefQuery" />

    <Label GridPane.columnIndex="1"
            GridPane.rowIndex="2"
           fx:id="aanbiedingenLabel"/>
    <TextField GridPane.columnIndex="2"
               GridPane.rowIndex="2"
               fx:id="inactiefTextField"/>
    <TextArea GridPane.columnIndex="3"
              GridPane.rowIndex="1"
              fx:id="inactief"/>


</GridPane>